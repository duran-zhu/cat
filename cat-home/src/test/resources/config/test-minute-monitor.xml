<monitor-rules>
	<rule id="two-minute">
		<config starttime="00:00" endtime="24:00">
			<condition minute="2">
					<subcondition type="DescPer">40</subcondition>
			</condition>
			<condition minute="2">
					<subcondition type="DescVal">80</subcondition>
			</condition>
		</config>
	</rule>
</monitor-rules>
